package br.com.universidade.modelo;

public class Aluno {

	private String nome;
	private String email;
	private int rm;
	
	//Sintaxe dos métodos
	//<modificador> <retorno do método> <nome do método> (<tipo Param> <nome para>
	
	public Aluno() {
		
	}
	
	public Aluno(String nome, String email, int rm) {
		this.nome=nome;
		this.email=email;
		this.rm=rm;
	}
	
	public String getALL() {
		
		return nome + "\n" + email + "\n" + rm;	}
	
	
	public void setALL(String nome, String email, int rm) {
		
		this.nome = nome;
		this.email = email;
		this.rm = rm;
		
	}




}
