package br.com.universidade.modelo;

public class Aluno {

private String nome;
private String email;
private int rm;


//Sintaxe dos métodos
//<modificador> <retorno do método> <nome do método> (<tipo Param> <nome para>


public void preencherNome(String pNome) {
	
	nome = pNome.toUpperCase();
}

public void preencherEmail(String pEmail) {
	
	email = pEmail.toUpperCase();
}

public void preencherEmail(int pRm) {
	
	rm = pRm;
}


public String retornarNome() {
	return nome;
}

public String retornarEmail() {
	return email;
}

public int retornarRm() {
	return rm;
}


}
