package br.com.universidade.modelo;

public class Aluno {

private String nome;
private String email;
private int rm;


//Sintaxe dos métodos
//<modificador> <retorno do método> <nome do método> (<tipo Param> <nome para>


public void preencherNome(String pNome) {
	
	nome = pNome;
}


public String retornarNome() {
	return nome;
}


}
